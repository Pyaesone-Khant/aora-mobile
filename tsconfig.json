{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		"./components/**/*.ts",
		".expo/types/**/*.ts",
		"./constants/**/*.ts",
		"./hooks/**/*.ts",
		"expo-env.d.ts",
		"app.d.ts",
		"constants/images.js"
	]
}
